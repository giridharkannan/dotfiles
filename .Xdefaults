URxvt.scrollBar: off
URxvt*termName:screen-256color
URxvt.foreground:white
URxvt.background:black
URxvt.iconFile:    /usr/share/icons/Clarity/scalable/apps/terminal.svg

URxvt*font:                 xft:Droid Sans Mono for Powerline:regular:size=11
URxvt*imFont:               xft:Droid Sans Mono for Powerline:regular:size=11
URxvt*boldFont:             xft:Droid Sans Mono for Powerline:bold:size=11
URxvt*italicFont:           xft:Droid Sans Mono for Powerline:italic:size=11
URxvt*boldItalicFont:       xft:Droid Sans Mono for Powerline:bold:italic:size=11

! black
URxvt*color0  : #000000
URxvt*color8  : #000000
! red
URxvt*color1  : #E52222
URxvt*color9  : #E52222
! green
URxvt*color2  : #A6E32D
URxvt*color10 : #A6E32D
! yellow
URxvt*color3  : #FC951E
URxvt*color11 : #FC951E
! blue
URxvt*color4  : #BF8DFF
URxvt*color12 : #BF8DFF
! magenta
URxvt*color5  : #FA250F
URxvt*color13 : #FA250F
! cyan
URxvt*color6  : #67D9F0
URxvt*color14 : #67D9F0
! white
URxvt*color7  : #F2F2F2
URxvt*color15 : #F2F2F2
